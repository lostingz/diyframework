<?xml version="1.0" encoding="UTF-8"?>
<web-app id="diyframework" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
   <display-name>diyframework</display-name>
	<filter>
		<filter-name>diyfilter</filter-name>
		<filter-class>com.z.diyframework.filter.DiyFilter</filter-class>
		<init-param>
			<param-name>bootClassName</param-name>
			<param-value>com.z.diyframework.test.MainTest</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>diyfilter</filter-name>
		<url-pattern>/*</url-pattern>
</filter-mapping>
</web-app>